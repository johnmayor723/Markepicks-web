<style>
    /* Styles */
    .border-orange {
        border: 2px solid #FE9801;
    }
    .btn-green {
        background-color: #2D7B30;
        color: white;
        font-size: 0.875rem; /* Smaller text size for the button */
    }
    .categories-container {
  display: grid;
  padding-right: 20px;
  padding-left: 20px;
  grid-template-columns: repeat(3, 1fr);
  gap: 20px;
  margin-top: 20px;
}

.category-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 0px;
  border-radius: 10px;
  text-align: center;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
  transition: all 0.3s ease;
  transform: scale(1);
}

.category-item:hover {
  background-color: #f4f4f4;
  box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
  transform: scale(1.05);
}
.category-name{
    font-size: 12px;
    color: gray;
}

.category-link {
  text-decoration: none;
  color: #333;
  display: block;
}

.category-icon {
  font-size: 40px;
  margin-bottom: 10px;
  transition: all 0.3s ease;
}

.category-item:hover .category-icon {
  color: #ff6f61; /* Change icon color on hover */
}

@media (max-width: 768px) {
  .categories-container {
    grid-template-columns: repeat(3, 1fr); /* 3 items per row on mobile */
  }
}

@media (max-width: 480px) {
  .categories-container {
    grid-template-columns: repeat(2, 1fr); /* 2 items per row on very small screens */
  }
}
    @media (max-width: 576px) {
        /* Display two images per row on mobile */
        .row-cols-sm-2 > * {
            flex: 0 0 50%;
            max-width: 50%;
        }
    }

.social-links {
    display: flex;
flex-direction: row;
    gap: 15px;
    justify-content: center;
    align-items: center;
    margin-top: 20px;
}

.social-links a {
    display: inline-block;
    width: 30px;
    height: 30px;
}

.social-links img {
    width: 100%;
    height: auto;
}
</style>
